<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorWhite">@color/CPwhite</item>
        <item name="colorYellow1">@color/CPyellow</item>
        <item name="colorYellow2">@color/CPyellow2</item>
        <item name="colorYellow3">@color/CPyellow3</item>
        <item name="colorOrange">@color/CPyellow3</item>
        <item name="android:windowTranslucentNavigation">false</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>